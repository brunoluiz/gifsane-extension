{
  "headers": [
    {
      "source" : "**/*",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        },
        {
          "key" : "Content-Security-Policy",
          "value" : "default-src 'self'; media-src * blob:"
        }
      ]
    }
  ]
}
